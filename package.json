{
  "name": "lsif-typescript-cli",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "git@github.com:Aaaaash/lsif-typescript-cli.git",
  "author": "Aaaaash <binshao54@gmail.com>",
  "license": "MIT",
  "bin": {
    "lsif-ts-cli": "./bin/lsif-ts-cli"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node --require ts-node/register --require tsconfig-paths/register src/index.ts",
    "test": "./node_modules/.bin/jest",
    "lint": "./node_modules/.bin/eslint --ext .ts ./ --ignore-pattern ./.eslintignore",
    "lint:fix": "./node_modules/.bin/eslint --ext .ts ./ --ignore-pattern ./.eslintignore --fix"
  },
  "dependencies": {
    "@types/commander": "^2.12.2",
    "@types/fs-extra": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "1.11.0",
    "@typescript-eslint/parser": "1.11.0",
    "commander": "2.19.0",
    "eslint": "6.0.1",
    "eslint-plugin-typescript": "0.14.0",
    "fs-extra": "^8.1.0",
    "typescript-eslint-parser": "22.0.0"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "typescript": "^3.5.3",
    "ts-node": "7.0.1",
		"tsconfig-paths": "3.7.0"
  }
}
